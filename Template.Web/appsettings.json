{
  "Urls": "http://0.0.0.0:8080",

  "AllowedHosts": "*",

  "FeatureManagement": {
    "CustomOption": true
  },

  "RateLimit": {
    "Api": {
      "Window": 1000,
      "PermitLimit": 1000,
      "QueueLimit": 20000
    }
  },

  "Server": {
    "ApiToken": "00000000000000000000000000000000",
    "LongTimeThreshold": 10000,
    "AllowHealth": [ "10.0.0.0/8", "127.0.0.1" ],
    "AllowMetrics": [ "10.0.0.0/8", "127.0.0.1" ]
  },

  "ConnectionStrings": {
    "Default": "Server=localhost;MultipleActiveResultSets=True;Initial Catalog=template;Integrated Security=True"
  },

  "Connector": {
    "SampleBaseUrl": "https://api.ipify.org/"
  },

  "Storage": {
    "Root": "..\\Storage"
  },

  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "Enrich": [ "FromLogContext", "WithThreadId" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore.DataProtection": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "..\\Log\\Template\\Template.Web_.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:HH:mm:ss.fff} {Level:u4} [{RequestId}] [{ThreadId}] {RequestPath} - {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }
}
