{
  //"Urls": "https://0.0.0.0",

  "AllowedHosts": "*",

  "ConnectionStrings": {
    "Default": "Server=localhost;MultipleActiveResultSets=True;Initial Catalog=template;Integrated Security=True"
  },

  "Server": {
    "ApiToken": "00000000000000000000000000000000",
    "LongTimeThreshold": 10000
  },

  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "Enrich": [ "FromLogContext", "WithThreadId" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore.DataProtection": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "..\\Log\\Template\\Template.Web_.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:HH:mm:ss.fff} {Level:u4} [{RequestId}] [{ThreadId}] {RequestPath} - {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }
}
